                </List>
                </value>
            </entry>
            <entry key="sailpoint.web.lcm.RequestPopulationBean">
                <value>
                <List>
                    <ColumnConfig dataIndex="id" fieldOnly="true" groupProperty="id" property="id" sortProperty="id" stateId="id"/>
                    <ColumnConfig dataIndex="IIQ_Selected" fieldOnly="true" stateId="IIQ_Selected"/>
                    <ColumnConfig dataIndex="name" flex="1.0" groupProperty="name" headerKey="idents_grid_hdr_name" hideable="true" property="name" sortProperty="name" sortable="true" stateId="name"/>
                    <ColumnConfig dataIndex="firstname" flex="1.0" groupProperty="firstname" headerKey="idents_grid_hdr_first_name" hideable="true" property="firstname" sortProperty="firstname" sortable="true" stateId="firstname"/>
                    <ColumnConfig dataIndex="lastname" flex="1.0" groupProperty="lastname" headerKey="idents_grid_hdr_last_name" hideable="true" property="lastname" sortProperty="lastname" sortable="true" stateId="lastname"/>
                    <ColumnConfig dataIndex="manager-displayName" flex="1.0" groupProperty="manager.displayName" headerKey="idents_grid_hdr_manager" hideable="true" property="manager.displayName" sortProperty="manager.displayName" sortable="true" stateId="manager-displayName"/>
                    <ColumnConfig dataIndex="displayName" fieldOnly="true" flex="1.0" groupProperty="displayName" headerKey="idents_grid_hdr_display_name" property="displayName" sortProperty="displayName" stateId="displayName"/>
                    <ColumnConfig dataIndex="organization" flex="1.0" groupProperty="organization" headerKey="Organization" hideable="true" property="organization" sortProperty="organization" sortable="true" stateId="organization"/>
                </List>
                </value>
            </entry>
        </Map>
        </Attributes>
    </UIConfig>
  </ImportAction>

  <!-- Configuration SystemConfiguration (merge option) -->
  <ImportAction name="merge">
    <Configuration name="SystemConfiguration">
        <Attributes>
        <Map>
            <entry key="enableProvisioningTransactionLog">
                <value>
                    <Boolean>true</Boolean>
                </value>
            </entry>
            <entry key="provisioningTransactionLogLevel" value="Success"/>
            <entry key="enableSyslog">
                <value>
                    <Boolean>true</Boolean>
                </value>
            </entry>
            <entry key="syslogLevel" value="WARN"/>
            <entry key="allowRolePropagation">
                <value>
                <Boolean>true</Boolean>
                </value>
            </entry>
            <entry key="identityControlsAssignedScope">
                <value>
                <Boolean>true</Boolean>
                </value>
            </entry>
            <entry key="scopingEnabled">
                <value>
                <Boolean>true</Boolean>
                </value>
            </entry>
            <entry key="unscopedObjectsGloballyAccessible">
                <value>
                <Boolean></Boolean>
                </value>
            </entry>
            <entry key="searchInputDefinitions">
                <value>
                <List>
                    <SearchInputDefinition description="task definition" headerKey="task definition" ignoreCase="true" inputType="Like" matchMode="ANYWHERE" name="taskDefinitionSearch" propertyName="name" propertyType="String" searchType="TaskResult"/>
                    <SearchInputDefinition description="task schedule" headerKey="task schedule" ignoreCase="true" inputType="Like" matchMode="ANYWHERE" name="taskScheduleSearch" propertyName="name" propertyType="String" searchType="TaskSchedule"/>
                    <SearchInputDefinition description="task result" headerKey="task result" ignoreCase="true" inputType="Like" matchMode="ANYWHERE" name="taskResultSearch" propertyName="name" propertyType="String" searchType="TaskResult"/>
                </List>
                </value>
            </entry>
        </Map>
        </Attributes>
    </Configuration>
  </ImportAction>

</sailpoint>
