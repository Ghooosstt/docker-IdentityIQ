<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE sailpoint PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<sailpoint>

  <!-- ObjectConfig Identity (merge option) -->
  <ImportAction name="merge">
    <ObjectConfig name="Identity">
        <Attributes>
            <Map>
                <entry key="scopeCorrelationAttribute" value="organization"/>
            </Map>
        </Attributes>
        <ObjectAttribute displayName="att_display_name" editMode="ReadOnly" name="displayName" standard="true" type="string">
            <AttributeSource name="displayName">
            <ApplicationRef>
                <Reference class="sailpoint.object.Application" name="HR File"/>
            </ApplicationRef>
            </AttributeSource>
        </ObjectAttribute>
        <ObjectAttribute displayName="att_email" editMode="ReadOnly" name="email" standard="true" type="string">
            <AttributeSource name="email">
            <ApplicationRef>
                <Reference class="sailpoint.object.Application" name="HR File"/>
            </ApplicationRef>
            </AttributeSource>
        </ObjectAttribute>
        <ObjectAttribute displayName="att_first_name" editMode="ReadOnly" name="firstname" standard="true" type="string">
            <AttributeSource name="firstname">
            <ApplicationRef>
                <Reference class="sailpoint.object.Application" name="HR File"/>
            </ApplicationRef>
            </AttributeSource>
        </ObjectAttribute>
        <ObjectAttribute displayName="att_last_name" editMode="ReadOnly" name="lastname" standard="true" type="string">
            <AttributeSource name="lastname">
            <ApplicationRef>
                <Reference class="sailpoint.object.Application" name="HR File"/>
            </ApplicationRef>
            </AttributeSource>
        </ObjectAttribute>
        <ObjectAttribute displayName="att_manager" editMode="ReadOnly" groupFactory="true" name="manager" standard="true" type="sailpoint.object.Identity">
            <AttributeSource name="manager">
            <ApplicationRef>
                <Reference class="sailpoint.object.Application" name="HR File"/>
            </ApplicationRef>
            </AttributeSource>
        </ObjectAttribute>
        <ObjectAttribute displayName="att_identity_type" editMode="ReadOnly" name="type" standard="true" type="string">
            <AttributeSource name="type">
            <ApplicationRef>
                <Reference class="sailpoint.object.Application" name="HR File"/>
            </ApplicationRef>
            </AttributeSource>
        </ObjectAttribute>
        <ObjectAttribute displayName="Phone Number" editMode="ReadOnly" name="phoneNumber" type="string">
            <AttributeSource name="phoneNumber">
            <ApplicationRef>
                <Reference class="sailpoint.object.Application" name="HR File"/>
            </ApplicationRef>
            </AttributeSource>
        </ObjectAttribute>
        <ObjectAttribute displayName="Country" editMode="ReadOnly" extendedNumber="1" name="country" type="string">
            <AttributeSource name="country">
            <ApplicationRef>
                <Reference class="sailpoint.object.Application" name="HR File"/>
            </ApplicationRef>
            </AttributeSource>
        </ObjectAttribute>
        <ObjectAttribute displayName="City" editMode="ReadOnly" name="city" type="string">
            <AttributeSource name="city">
            <ApplicationRef>
                <Reference class="sailpoint.object.Application" name="HR File"/>
            </ApplicationRef>
            </AttributeSource>
        </ObjectAttribute>
        <ObjectAttribute displayName="Organization" editMode="ReadOnly" extendedNumber="2" name="organization" type="string">
            <AttributeSource name="organization">
            <ApplicationRef>
                <Reference class="sailpoint.object.Application" name="HR File"/>
            </ApplicationRef>
            </AttributeSource>
        </ObjectAttribute>
        <ObjectAttribute displayName="End Date" editMode="ReadOnly" name="endDate" type="string">
            <AttributeSource name="endDate">
            <ApplicationRef>
                <Reference class="sailpoint.object.Application" name="HR File"/>
            </ApplicationRef>
            </AttributeSource>
        </ObjectAttribute>
        <ObjectAttribute displayName="att_inactive" editMode="ReadOnly" name="inactive" standard="true" type="boolean">
            <AttributeSource name="AppRule: Custom-Rule-inactive HR File">
            <ApplicationRef>
                <Reference class="sailpoint.object.Application" name="HR File"/>
            </ApplicationRef>
            <RuleRef>
                <Reference class="sailpoint.object.Rule" name="Custom-Rule-inactive"/>
            </RuleRef>
            </AttributeSource>
        </ObjectAttribute>
    </ObjectConfig>
  </ImportAction>

  <!-- UIConfig (merge option) -->
  <ImportAction name="merge">
    <UIConfig name="UIConfig">
        <Attributes>
        <Map>
            <entry key="identityViewAttributes" value="name,firstname,lastname,displayName,email,phoneNumber,manager,type,country,city,organization,endDate,inactive"/>
            <entry key="uiRequestAccessIdentityCard">
                <value>
                <List>
                    <ColumnConfig dataIndex="id" fieldOnly="true" groupProperty="id" property="id" sortProperty="id" stateId="id"/>
                    <ColumnConfig dataIndex="displayName" fieldOnly="true" groupProperty="displayName" property="displayName" sortProperty="displayName" sortable="true" stateId="displayName"/>
                    <ColumnConfig dataIndex="name" groupProperty="name" headerKey="ui_access_username" property="name" sortProperty="name" stateId="name"/>
                    <ColumnConfig dataIndex="managerName" groupProperty="manager.displayName" headerKey="ui_access_manager" property="manager.displayName" sortProperty="manager.displayName" stateId="managerName"/>
                    <ColumnConfig dataIndex="country" groupProperty="country" headerKey="Country" property="country" sortProperty="country" stateId="country"/>
                    <ColumnConfig dataIndex="organization" groupProperty="organization" headerKey="Organization" property="organization" sortProperty="organization" stateId="organization"/>
                </List>
                </value>
            </entry>
            <entry key="uiManageAccountsIdentityCard">
                <value>
                <List>
                    <ColumnConfig dataIndex="id" fieldOnly="true" groupProperty="id" property="id" sortProperty="id" stateId="id"/>
                    <ColumnConfig dataIndex="displayName" fieldOnly="true" groupProperty="displayName" property="displayName" sortProperty="displayName" sortable="true" stateId="displayName"/>
                    <ColumnConfig dataIndex="name" groupProperty="name" headerKey="ui_access_username" property="name" sortProperty="name" stateId="name"/>
                    <ColumnConfig dataIndex="managerName" groupProperty="manager.displayName" headerKey="ui_access_manager" property="manager.displayName" sortProperty="manager.displayName" stateId="managerName"/>
                    <ColumnConfig dataIndex="country" groupProperty="country" headerKey="Country" property="country" sortProperty="country" stateId="country"/>
                    <ColumnConfig dataIndex="organization" groupProperty="organization" headerKey="Organization" property="organization" sortProperty="organization" stateId="organization"/>
                </List>
                </value>
            </entry>
            <entry key="uiManageEditIdentityCard">
                <value>
                <List>
                    <ColumnConfig dataIndex="id" fieldOnly="true" groupProperty="id" property="id" sortProperty="id" stateId="id"/>
                    <ColumnConfig dataIndex="displayName" fieldOnly="true" groupProperty="displayName" property="displayName" sortProperty="displayName" sortable="true" stateId="displayName"/>
                    <ColumnConfig dataIndex="name" groupProperty="name" headerKey="ui_access_username" property="name" sortProperty="name" stateId="name"/>
                    <ColumnConfig dataIndex="managerName" groupProperty="manager.displayName" headerKey="ui_access_manager" property="manager.displayName" sortProperty="manager.displayName" stateId="managerName"/>
                    <ColumnConfig dataIndex="country" groupProperty="country" headerKey="Country" property="country" sortProperty="country" stateId="country"/>
                    <ColumnConfig dataIndex="organization" groupProperty="organization" headerKey="Organization" property="organization" sortProperty="organization" stateId="organization"/>
                </List>
                </value>
            </entry>
            <entry key="uiManagePasswordsIdentityCard">
                <value>
                <List>
                    <ColumnConfig dataIndex="id" fieldOnly="true" groupProperty="id" property="id" sortProperty="id" stateId="id"/>
                    <ColumnConfig dataIndex="displayName" fieldOnly="true" groupProperty="displayName" property="displayName" sortProperty="displayName" sortable="true" stateId="displayName"/>
                    <ColumnConfig dataIndex="name" groupProperty="name" headerKey="ui_access_username" property="name" sortProperty="name" stateId="name"/>
                    <ColumnConfig dataIndex="managerName" groupProperty="manager.displayName" headerKey="ui_access_manager" property="manager.displayName" sortProperty="manager.displayName" stateId="managerName"/>
                    <ColumnConfig dataIndex="country" groupProperty="country" headerKey="Country" property="country" sortProperty="country" stateId="country"/>
                    <ColumnConfig dataIndex="organization" groupProperty="organization" headerKey="Organization" property="organization" sortProperty="organization" stateId="organization"/>
                </List>
                </value>
            </entry>
            <entry key="uiManageViewIdentityAttributesCard">
                <value>
                <List>
                    <ColumnConfig dataIndex="id" fieldOnly="true" groupProperty="id" property="id" sortProperty="id" stateId="id"/>
                    <ColumnConfig dataIndex="displayName" fieldOnly="true" groupProperty="displayName" property="displayName" sortProperty="displayName" sortable="true" stateId="displayName"/>
                    <ColumnConfig dataIndex="name" groupProperty="name" headerKey="ui_access_username" property="name" sortProperty="name" stateId="name"/>
                    <ColumnConfig dataIndex="managerName" groupProperty="manager.displayName" headerKey="ui_access_manager" property="manager.displayName" sortProperty="manager.displayName" stateId="managerName"/>
                    <ColumnConfig dataIndex="country" groupProperty="country" headerKey="Country" property="country" sortProperty="country" stateId="country"/>
                    <ColumnConfig dataIndex="organization" groupProperty="organization" headerKey="Organization" property="organization" sortProperty="organization" stateId="organization"/>
                </List>
                </value>
            </entry>
            <entry key="identityTableColumns">
                <value>
                <List>
                    <ColumnConfig dataIndex="id" fieldOnly="true" groupProperty="id" property="id" sortProperty="id" stateId="id"/>
                    <ColumnConfig dataIndex="name" groupProperty="name" headerKey="idents_grid_hdr_name" hideable="true" property="name" sortProperty="name" sortable="true" stateId="name"/>
                    <ColumnConfig dataIndex="firstname" groupProperty="firstname" headerKey="idents_grid_hdr_first_name" hideable="true" property="firstname" sortProperty="firstname" sortable="true" stateId="firstname"/>
                    <ColumnConfig dataIndex="lastname" groupProperty="lastname" headerKey="idents_grid_hdr_last_name" hideable="true" property="lastname" sortProperty="lastname" sortable="true" stateId="lastname"/>
                    <ColumnConfig dataIndex="manager-displayName" groupProperty="manager.displayName" headerKey="idents_grid_hdr_manager" hideable="true" property="manager.displayName" sortProperty="manager.displayName" sortable="true" stateId="manager-displayName"/>
                    <ColumnConfig dataIndex="type" groupProperty="type" headerKey="idents_grid_hdr_type" hideable="true" property="type" sortProperty="type" sortable="true" stateId="type"/>
                    <ColumnConfig dataIndex="organization" groupProperty="organization" headerKey="Organization" hideable="true" property="organization" sortProperty="organization" sortable="true" stateId="organization"/>
                    <ColumnConfig dataIndex="assignedRoleSummary" groupProperty="assignedRoleSummary" headerKey="idents_grid_hdr_assigned_roles" hideable="true" property="assignedRoleSummary" sortProperty="assignedRoleSummary" stateId="assignedRoleSummary"/>
                    <ColumnConfig dataIndex="bundleSummary" groupProperty="bundleSummary" headerKey="idents_grid_hdr_detected_roles" hideable="true" property="bundleSummary" sortProperty="bundleSummary" stateId="bundleSummary"/>
                    <ColumnConfig dataIndex="scorecard-compositeScore" groupProperty="scorecard.compositeScore" headerKey="idents_grid_hdr_composite_score" hideable="true" property="scorecard.compositeScore" renderer="SailPoint.Define.Grid.Identity.renderScore" sortProperty="scorecard.compositeScore" sortable="true" stateId="scorecard-compositeScore"/>
