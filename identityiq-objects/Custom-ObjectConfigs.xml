                    <ColumnConfig dataIndex="administrator-displayName" groupProperty="administrator.displayName" headerKey="idents_grid_hdr_administrator" hidden="true" hideable="true" property="administrator.displayName" sortProperty="administrator.displayName" sortable="true" stateId="administrator-displayName"/>
