            </Map>
        </Attributes>
        <Section>
            <Field displayName="con_prov_policy_ldap_description" name="description" reviewRequired="true" type="string"/>
        </Section>
        </Form>
        <Form name="edit group" objectType="nisNetgroup" type="Update">
        <Attributes>
            <Map>
            <entry key="pageTitle" value="edit group"/>
            </Map>
        </Attributes>
        <Section>
            <Field displayName="con_prov_policy_ldap_description" name="description" reviewRequired="true" type="string"/>
        </Section>
        </Form>
    </ProvisioningForms>
